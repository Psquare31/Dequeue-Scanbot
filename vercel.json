{
  "rewrites": [
    {
      "source": "/api/products/:path*",
      "destination": "https://deque-scanbot-backend-testing.vercel.app/api/products/:path*"
    },
    {
      "source": "/api/recommendations/:path*",
      "destination": "https://deque-scanbot-backend-testing.vercel.app/api/recommendations/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}

